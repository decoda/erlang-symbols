{
    "name": "erlang-symbols",
    "displayName": "erlang-symbols",
    "description": "VSCode Erlang symbols search methods",
    "version": "0.0.2",
    "publisher": "tangj",
    "icon": "erlang.png",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "onLanguage:erlang"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/decoda/erlang-symbols"
    },
    "main": "./out/extension",
    "contributes": {
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42",
        "vscode-uri": "^1.0.6"
    }
}
